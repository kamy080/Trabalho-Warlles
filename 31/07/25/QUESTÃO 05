#include <stdio.h>

int main() {
    int num;
    printf("Digite um numero: ");
    scanf("%d", &num);

    if ((num % 2 == 0 && num % 3 != 0) ||
        (num % 3 == 0 && num % 2 != 0)) {
        printf("Atende a condicao\n");
    } else {
        printf("Nao atende a condicao\n");
    }
    return 0;
}

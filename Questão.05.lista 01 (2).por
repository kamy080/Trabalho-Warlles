programa {
  funcao inicio() {
    // declaraçao de variaveis 
    real altura, peso_ideal
    // declaraçao de constantes 
    const real k= 72.7
    const inteiro B= 38 
    // enrada de dados 
    escreva (" informe altura: ")
    leia( altura)
    // processamento 
    peso_ideal =( k*altura-B)
    // saida de dados 
    escreva (" O peso_ideal é: ",peso_ideal)
  
  }
}
